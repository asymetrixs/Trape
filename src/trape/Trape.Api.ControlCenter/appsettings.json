{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "trape-db": "Server=<server>;Port=<port>;uid=<uid>;pwd=<pwd>;database=<database>;enlist=true;Pooling=true;SSL Mode=Require;Trust Server Certificate=true"
  },
  "Serilog": {
    "MinimumLevel": "Debug",
    "LogFileLocation": "trader-.log",
    "OutputTemplateConsole": "[{Level:u3}] {Message:j}{NewLine}",
    "OutputTemplateFile": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] [{SourceContext}] {Message:lj} {Properties}{NewLine}{Exception}"
  }
}
